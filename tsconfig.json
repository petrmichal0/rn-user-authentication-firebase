{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "lib": ["es2017"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*", "**/*.ts", "**/*.tsx"],
  "extends": "expo/tsconfig.base"
}
